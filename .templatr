{
  "name": "Quest Mod Template",
  "author": "Lauriethefish",
  "src": "./template",
  "description": "Template for creating QuestPatcher/BMBF mods for the Oculus Quest.",
  "placeholders": [
    {"match": "C:/Qmod/Android", "prompt": "The path to the android NDK", "cachable": true},
    {"match": "BedroomParty-LB", "prompt": "The ID of the mod"},
    {"match": "BedroomParty-LB", "prompt": "The name of the mod"},
    {"match": "#{author}", "prompt": "The author of the mod"},
    {"match": "A plugin that piggybacks off of Beat Leader for the Bedroom Party discord server", "prompt": "The description of the mod"}
  ]
}
